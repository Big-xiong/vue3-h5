    "postcss-px-to-viewport-update": "^1.2.0",
    "pre-commit": "^1.2.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rollup-plugin-visualizer": "^5.6.0",
    "rsyncwrapper": "^3.0.1",
    "shelljs": "^0.8.5",
    "simple-git": "^3.7.1",
    "tinify": "^1.6.1",
    "unplugin-auto-import": "^0.7.1",
