{
  "name": "overseas-h5",
  "version": "0.0.1",
  "description": "Use vue3 vite eslint SSR vuex vue-router vant less",
  "author": "AAGame",
  "scripts": {
    "dev": "vite",
    "dev:sport": "cross-env TARGET=sport vite",
    "micro": "node --trace-warnings scripts/micro.js --unsafe-perm",
    "start": "node scripts/cli.mjs",
    "sever": "vite --force --host --mode development",
    "favicon": "cross-env NODE_ENV=production node scripts/favicon",
    "build": "node scripts/build",
    "build:spa": "vite build",
    "build:sport": "cross-env TARGET=sport node scripts/buildTest.mjs",
    "build:micro": "node scripts/patches add && npm install && mode=micro vite build --mode micro && node scripts/patches && cross-env NODE_ENV=production node scripts/seo",
    "build:test": "node scripts/buildTest",
    "test": "cross-env SOURCE_MAP=true npm run build && cross-env NODE_ENV=test node server",
    "lint": "eslint . --fix --ext .js,.mjs,.ts,.vue --ignore-path .gitignore",
    "postinstall": "patch-package",
    "re": "rm -rf node_modules/.vite && npm run start",
    "init": "node scripts/init.js",
    "tpl": "node scripts/submodule",
    "replace": "node scripts/autoGit.js",
    "check-code": "node scripts/checkCode"
  },
  "pre-commit": [
    "check-code"
  ],
  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "@vueuse/head": "^0.6.0",
    "ahooks-vue": "^0.12.7",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "dom-to-image": "^2.6.0",
    "echarts": "^5.2.1",
    "google-translate-api": "^2.3.0",
    "html2canvas": "^1.4.1",
    "inquirer": "^8.2.4",
    "js-cookie": "^3.0.1",
    "md5": "^2.3.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "qrcode": "^1.5.0",
    "smoothscroll-polyfill": "^0.4.4",
    "socket.io-client": "^2.3.0",
    "vant": "3.4.9",
    "video.js": "^7.20.1",
    "vivus": "^0.4.6",
    "vue": "^3.2.33",
    "vue-awesome-swiper": "^3.1.3",
    "vue-countupjs": "^1.0.0",
    "vue-i18n": "^9.1.10",
    "vue-list-marquee": "^1.0.48",
    "vue-request": "^1.2.4",
    "vue-router": "^4.0.13",
    "vue3-clipboard": "^1.0.0",
